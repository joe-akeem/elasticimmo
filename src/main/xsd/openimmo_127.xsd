<!-- Technische Hinweise zu Datentypen
 An instance of a datatype ·boolean· can have the following legal literals {true, false, 1, 0}.  
 An instance of a datatype ·decimal· looks like  123.45  not! 123,45 -->

<!-- <xsd:include schemaLocation="opim-iso.xsd"/>import von land und waehrung. Kann auch getrennt werden  -->
	
	<xsd:element name="openimmo">
		<xsd:annotation>
			<xsd:appinfo>0</xsd:appinfo>
			<xsd:documentation>Dokument Element</xsd:documentation>
			<xsd:documentation xml:lang="en">Root Element</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="uebertragung"/>
				<xsd:element ref="anbieter" maxOccurs="unbounded"/>
				<xsd:element ref="user_defined_simplefield" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="user_defined_anyfield" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<!-- ========== ROOT ============= -->
	
	<xsd:element name="uebertragung">
		<xsd:annotation>
			<xsd:documentation>Übertragungsangaben</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="art" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="ONLINE"/>
						<xsd:enumeration value="OFFLINE"/>
					</xsd:restriction>
				</xsd:simpleType>
				<!--Je nach Art der Übertragung wird eine Antwort erwartet/zurückgegeben-->
			</xsd:attribute>
			<xsd:attribute name="umfang" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="TEIL"/>
						<xsd:enumeration value="VOLL"/>
					</xsd:restriction>
				</xsd:simpleType>
				<!--Umfang gibt an ob es sich um einen Vollabgleich, welchee den kompletten Datenbestand wiederspiegelt 
					oder um einen Teilabgleich, wo nur einzufügende, zu ändernde oder zu löschen Objekte übertragen werden (inkrementell), 
					handelt-->
			</xsd:attribute>
			<xsd:attribute name="modus"><!--$V120-->
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="NEW"/>
						<xsd:enumeration value="CHANGE"/>
						<xsd:enumeration value="DELETE"/>
					</xsd:restriction>
				</xsd:simpleType>				
				<!--Informierender Charakter für Löschen ohne <immo> ELement-->
			</xsd:attribute>
			<xsd:attribute name="version" type="xsd:string" use="required"/><!--$V120-->
			<!--Version = OPENIMMO! Version im Format: Major.Minor.Revision-->	
			<xsd:attribute name="sendersoftware" type="xsd:string" use="required"/>
			<xsd:attribute name="senderversion" type="xsd:string" use="required"/><!--$V120-->
			<!--Version der Exportsoftware: Major.Minor-->
			<xsd:attribute name="techn_email" type="xsd:string" use="optional"/>
			<!--Die Email des techn. Ansprechpartners, Adresse für die Antwort (log-file) des Imports-->
			<xsd:attribute name="regi_id" type="xsd:string" use="optional"/><!--$V120-->
			<!--Registrieungskey aus der Lizenzvereinbarung, falls erfolgt-->
			<xsd:attribute name="timestamp" type="xsd:dateTime" use="optional"/><!--$V122-->			
		</xsd:complexType>
	</xsd:element>
	
	<xsd:element name="anbieter">
		<xsd:annotation>
			<xsd:documentation>Anbieterangaben</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="anbieternr" minOccurs="0"/>
				<xsd:element ref="firma"/>
				<xsd:element ref="openimmo_anid"/>
				<xsd:element ref="lizenzkennung" minOccurs="0"/><!--$V120-->
				<xsd:element ref="anhang" minOccurs="0"/>
				<xsd:element ref="immobilie" minOccurs="0" maxOccurs="unbounded"/><!--$V120 min=0-->
				<xsd:element ref="impressum" minOccurs="0"/>
				<xsd:element ref="impressum_strukt" minOccurs="0"/><!--$V120-->
				<xsd:element ref="user_defined_simplefield" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="user_defined_anyfield" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="user_defined_extend" minOccurs="0" maxOccurs="unbounded"/><!--$V120-->				
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:element name="immobilie">
		<xsd:annotation>
			<xsd:documentation>Angaben einer einzelnen Immobile</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="objektkategorie"/>
				<xsd:element ref="geo"/>
