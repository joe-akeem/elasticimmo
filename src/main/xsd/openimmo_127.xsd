	<!-- MIME Types gemäß http://www.iana.org/assignments/media-types/index.html   -->
	<xsd:element name="format" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Dateiformat des Anhanges</xsd:documentation>
			<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>

