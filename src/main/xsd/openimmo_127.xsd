				<xsd:element ref="anhaenge" minOccurs="0"/>
				<xsd:element ref="verwaltung_objekt" minOccurs="0"/>
				<xsd:element ref="verwaltung_techn"/>
				<xsd:element ref="user_defined_simplefield" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="user_defined_anyfield" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="user_defined_extend" minOccurs="0" maxOccurs="unbounded"/><!--$V120-->
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:element name="objektkategorie">
