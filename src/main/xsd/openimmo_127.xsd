	<!-- $V124   Optionale Angabe von Daten für die Prüfung auf ein Update     -->
	<xsd:element name="check"  >
		<xsd:annotation>
			<xsd:documentation>Angabe von Daten für die Prüfung auf ein Update </xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>		
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="xsd:dateTime">

			<xsd:attribute name="ctype" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="MD5"/> <!--  Enthält eine MD5 Prüfsumme -->
						<xsd:enumeration value="DATETIME"/> <!-- Enthält eine DateTime Angabe der Datei -->
						<xsd:enumeration value="ETAG"/> <!--  Kann  über das HTTP E-TAG geprüft werden  -->
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>

	</xsd:element>

	
	
	<xsd:element name="daten">
		<xsd:annotation>
			<xsd:documentation>Anhangdaten</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice>
				<xsd:element ref="pfad" minOccurs="0"/>
				<xsd:element ref="anhanginhalt" minOccurs="0"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:element name="anhanginhalt" type="xsd:base64Binary">
		<xsd:annotation>
			<xsd:documentation>Base64 codierter Inhalt bei Binärdaten</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="pfad" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Pfad der angehängten Datei, relativ zur XML-Datei</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="objektadresse_freigeben" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Darf die Adresse des Objektes freigeben/angezeigt werden, 
							   fehlende Angabe wird als 'Ja' interpretiert
			</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="verfuegbar_ab" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Ab wann ist die Immobilie verfügbar/beziehbar/zur Übernahme bereit, 
							   Inhalt Text wie 'ab sofort' aber auch eine Datumsangabe
			</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="abdatum" type="xsd:date">
		<xsd:annotation>
			<xsd:documentation>Ab wann ist die Immobilie verfügbar/beziehbar/zur Übernahme bereit, 
							   Inhalt Datum; Datumsformat YYYY-MM-DD (ISO 8601) zusätzlich zu 'verfuegbar_ab'
			</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="bisdatum" type="xsd:date">
		<xsd:annotation>
			<xsd:documentation>Bis wann ist die Immobilie verfügbar/beziehbar/zur Übernahme bereit, 
							   Inhalt Datum; Datumsformat YYYY-MM-DD (ISO 8601) anstelle des urspr. 'verfuegbar_bis', vorrangig bei WAZ - Objekten
			</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="min_mietdauer">
		<xsd:annotation>
			<xsd:documentation>Mindestzeitraum für den die Immobilie gemietet werden muss, Optionen nicht kombinierbar, vorrangig bei WaZ</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:attribute name="min_dauer">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="TAG"/>
						<xsd:enumeration value="WOCHE"/>
						<xsd:enumeration value="MONAT"/>
						<xsd:enumeration value="JAHR"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:element name="max_mietdauer">
		<xsd:annotation>
			<xsd:documentation>Maximalzeitraum für den die Immobilie gemietet werdenkann, Optionen nicht kombinierbar, vorrangig bei WaZ</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:attribute name="max_dauer">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="TAG"/>
						<xsd:enumeration value="WOCHE"/>
						<xsd:enumeration value="MONAT"/>
						<xsd:enumeration value="JAHR"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:element name="versteigerungstermin" type="xsd:date">
		<xsd:annotation>
			<xsd:documentation>Termin der Versteigerung, Datumsformat YYYY-MM-DD (ISO 8601)</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="wbs_sozialwohnung" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>WBS erforderlich/Sozialwohnung?</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="vermietet" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Ist das Objekt noch/bereits vermietet?</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="anbieternr" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Kundennummer des Anbieters (Anbieterschlüssel) bei dem Immobilienportal, an das übertragen wird</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="objektnr_intern" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Eindeutiger Identifikator, mit der das generierende bzw. auch das importierende System den Datensatz identifiziert, Pflichtfeld</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="objektnr_extern" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Identifikator, der im Expose dargestellt wird. Auf diesen bezieht sich dann eine Kundenanfrage. 
							   Kann mit der internen Objektnummmer identisch sein (für Systeme, die nur einen Nummernkreis verwalten), Pflichtfeld
			</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="gruppennummer" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Gruppennummer/Mutterobjekt falls Objekte gruppiert sind oder ein gemeinsames Mutterobjekt</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="zugang" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Ab wann besteht Zugang zur Wohnung, allg. Text</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="aktiv_von" type="xsd:date">
		<xsd:annotation>
			<xsd:documentation>Ab wann soll das Objekt im Immobilienportal aktiv sein, Datumsformat YYYY-MM-DD (ISO 8601)</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="aktiv_bis" type="xsd:date">
		<xsd:annotation>
			<xsd:documentation>Bis wann soll das Objekt im Immobilienportal aktiv sein, Datumsformat YYYY-MM-DD (ISO 8601)</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="laufzeit" type="xsd:decimal">
		<xsd:annotation>
			<xsd:documentation>(Rest-)Laufzeit der Erbpacht in Jahren</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="max_personen" type="xsd:positiveInteger">
		<xsd:annotation>
			<xsd:documentation>Max. Personenzahl, die in dem Objekt wohnen/schlafen können</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="nichtraucher" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Wird das Objekt nur an Nichtraucher vermietet?</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="haustiere" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Sind Haustiere gestattet?</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="geschlecht">
		<xsd:annotation>
			<xsd:documentation>Soll das Objekt nur an Frauen bzw. nur an Männer vermietet werden, 
							   fehlende Angabe wird als 'Ja' interpretiert
			</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="geschl_attr">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="EGAL"/>
						<xsd:enumeration value="NUR_MANN"/>
						<xsd:enumeration value="NUR_FRAU"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:element name="denkmalgeschuetzt" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Steht das Objekt unter Denkmalschutz?</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="stand_vom" type="xsd:date">
		<xsd:annotation>
			<xsd:documentation>Datum der letzten Änderung im exportierenden System, Datumsformat YYYY-MM-DD (ISO 8601), Pflichtfeld</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="kennung_ursprung" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Kennung des ursprünglichen Datenlieferanten (generierenden/ursprung Systems)</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="openimmo_obid" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Eindeutiger Identifikator der Immobilie innerhalb aller Immobilienbörsen-Portale, 
							   der Aufbau ist definiert (s. Protokoll v. 28.11.01), wird aber nicht durch die XSD überprüft, 
							   da hier in absehbarer Zeit durch Verwendung von Verschlüsselung eine Änderungen stattfindet
			</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="weitergabe_generell" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Darf die Immobilie an andere Börsenportale weitergegeben werden</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="weitergabe_negativ" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>ExpliziteListe der Börsenportale, an die das Objekt nicht weiter gegeben werden darf, 
							   Angabe der der jeweiligen Domänen mit Semikolon separiert. 
							   Falls eine Domäne hier und in weitergabe_positiv aufgeführt wird, 
							   hat die Angabe in diesem Feld den Vorrang!
			</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="weitergabe_positiv" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Explizite Liste der Börsenportale, an die das Objekt weiter gegeben werden darf, 
							   Angabe der der jeweiligen Domänen mit Semikolon separiert.
			</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="gruppen_kennung" type="xsd:string"><!-- $V125 -->
		<xsd:annotation>
			<xsd:documentation>Frei wählbare alphanumerische Kennung um Objekte mit gleicher Kennung
zu gruppieren. Anwendung z.b. in Neubau Projekten.  Siehe  auch Element "master".
			</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	

	<xsd:element name="sprache" type="xsd:string"><!-- $V125 -->
		<xsd:annotation>
			<xsd:documentation>Optionale Angabe der Sprache, falls nicht Deutsch (de-DE)
Language Code als zusammengesetzte Kennung gemäß RFC 1766. D.h. Sprache gemäß ISO 639-1 und Länderkennung nach ISO 3166
(Großbuchstaben) z.B. de-DE oder de-CH, en-US amerikanisches Englisch, en-GB Britisches Englisch.
			</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	




	<xsd:element name="master" ><!-- $V125 -->
		<xsd:annotation>
			<xsd:documentation>Frei wählbare alphanumerische Kennung um Objekte einem Übergeordneten Objekt zuzuordnen. 
Das Eltern Objekte hat in  "gruppen_kennung" die gleiche ID wie "master". Anwendung z.b. in Neubau Projekten.
Damit die Anzeige des Master Objektes gesteuert werden kann, wird im Master ein Flag
 visible eingesetzt. Das Attribut ist dann zwingend anzugeben
			</xsd:documentation>
			<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	<xsd:complexType mixed="true">
			<xsd:attribute name="visible"  type="xsd:boolean" use="required" />
	</xsd:complexType>

	</xsd:element>




	<xsd:element name="als_ferien" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Ist das Objekt auch als Ferienimmoblie geeignet?</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="gewerbliche_nutzung" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Ist zusätzlich eine gewerbliche Nutzung möglich? - evtl. Überschneidung mit Nutzungsart</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="branchen" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Für welche Branchen ist eine zusätzliche gewerbliche Nutzung möglich?</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="hochhaus" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Befindet sich die Wohnung in einem Hochhaus,</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="user_defined_simplefield">
		<xsd:annotation>
			<xsd:documentation>Benutzerdefinierte Angaben</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:attribute name="feldname" type="xsd:string" use="required"/>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:element name="user_defined_anyfield">
		<xsd:complexType mixed="true">
			<xsd:sequence minOccurs="0" maxOccurs="unbounded">
				<xsd:any processContents="lax"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:element name="user_defined_extend">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="feld" minOccurs="0" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="name" type="xsd:string"/>
							<xsd:element name="wert" type="xsd:string"/>
							<xsd:element name="typ" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
							<xsd:element name="modus" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:element name="aktion">
		<xsd:annotation>
			<xsd:documentation>Aktion für Objekt. Wenn nicht vorhanden, dann "ADD", als neu.
			Change= Update der Objektdaten,   Delete = Löschen des Objektes
			Referenz=  Die Möglichkeit Objekte in Portalen als Verkauft oder Archiv zu  definieren.
			
			</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="aktionart">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="CHANGE"/>
						<xsd:enumeration value="DELETE"/>
						<xsd:enumeration value="REFERENZ"/><!-- $V127  -->

					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:element name="zimmer">
		<xsd:annotation>
			<xsd:documentation>Objekart / Typ f. Zimmer</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="zimmertyp">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="ZIMMER"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:element name="wohnung"> 
		<xsd:annotation>
			<xsd:documentation>Objektart / Typ f. Wohnungen</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="wohnungtyp">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="DACHGESCHOSS"/>
						<xsd:enumeration value="MAISONETTE"/>
						<xsd:enumeration value="LOFT-STUDIO-ATELIER"/>
						<xsd:enumeration value="PENTHOUSE"/>
						<xsd:enumeration value="TERRASSEN"/>
						<xsd:enumeration value="ETAGE"/>
						<xsd:enumeration value="ERDGESCHOSS"/>
						<xsd:enumeration value="SOUTERRAIN"/>
						<xsd:enumeration value="APARTMENT"/><!-- $V123-->
						<xsd:enumeration value="FERIENWOHNUNG"/><!-- $V123-->
						<xsd:enumeration value="GALERIE"/><!-- $V123-->
						<xsd:enumeration value="ROHDACHBODEN"/><!-- $V124-->
						<xsd:enumeration value="ATTIKAWOHNUNG"/><!-- $V125-->

						<xsd:enumeration value="KEINE_ANGABE"/><!-- $V122-->
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:element name="haus"> 
		<xsd:annotation>
			<xsd:documentation>Objektart / Typ f. Haus</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="haustyp">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="REIHENHAUS"/>
						<xsd:enumeration value="REIHENEND"/>
						<xsd:enumeration value="REIHENMITTEL"/>
						<xsd:enumeration value="REIHENECK"/>
						<xsd:enumeration value="DOPPELHAUSHAELFTE"/>
						<xsd:enumeration value="EINFAMILIENHAUS"/>
						<xsd:enumeration value="STADTHAUS"/>
						<xsd:enumeration value="BUNGALOW"/>
						<xsd:enumeration value="VILLA"/>
						<xsd:enumeration value="RESTHOF"/>
						<xsd:enumeration value="BAUERNHAUS"/>
						<xsd:enumeration value="LANDHAUS"/>
						<xsd:enumeration value="SCHLOSS"/>
						<xsd:enumeration value="ZWEIFAMILIENHAUS"/>
						<xsd:enumeration value="MEHRFAMILIENHAUS"/>
						<xsd:enumeration value="FERIENHAUS"/>
						<xsd:enumeration value="BERGHUETTE"/>
						<xsd:enumeration value="CHALET"/>
						<xsd:enumeration value="STRANDHAUS"/>
						<xsd:enumeration value="LAUBE-DATSCHE-GARTENHAUS"/>
						<!--$V123:   Krankenhaus  verschoben zu sonstige -->
						<!--$V123:   PFLEGEHEIM, SANATORIUM, SENIORENHEIM, BETREUTES-WOHNEN verschoben zu zinshaus_renditeobjekt-->
						<xsd:enumeration value="APARTMENTHAUS"/><!--$V123-->
						<xsd:enumeration value="BURG"/><!--$V123-->
						<xsd:enumeration value="HERRENHAUS"/><!--$V123-->
						<xsd:enumeration value="FINCA"/><!--$V123-->
						<xsd:enumeration value="RUSTICO"/><!--$V123-->
						<xsd:enumeration value="FERTIGHAUS"/><!--$V125-->
						<xsd:enumeration value="KEINE_ANGABE"/><!-- V122-->
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>		
	</xsd:element>
	
	<xsd:element name="grundstueck">
		<xsd:annotation>
			<xsd:documentation>Objektart / Typ f. Grundstücke</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="grundst_typ">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="WOHNEN"/>
						<xsd:enumeration value="GEWERBE"/>
						<xsd:enumeration value="INDUSTRIE"/>
						<xsd:enumeration value="LAND_FORSTWIRSCHAFT"/>
						<xsd:enumeration value="FREIZEIT"/>
						<xsd:enumeration value="GEMISCHT"/>
						<xsd:enumeration value="GEWERBEPARK"/><!--$V122-->
						<xsd:enumeration value="SONDERNUTZUNG"/>
						<xsd:enumeration value="SEELIEGENSCHAFT"/><!--$V124-->

					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:element name="buero_praxen">
		<xsd:annotation>
			<xsd:documentation>Objektart / Typ f. Büro/Praxen</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="buero_typ">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="BUEROFLAECHE"/>
						<xsd:enumeration value="BUEROHAUS"/>
						<xsd:enumeration value="BUEROZENTRUM"/><!--$V122-->
						<xsd:enumeration value="LOFT_ATELIER"/><!--$V122-->
						<xsd:enumeration value="PRAXIS"/>
						<xsd:enumeration value="PRAXISFLAECHE"/><!--$V122-->
						<xsd:enumeration value="PRAXISHAUS"/><!--$V122-->
						<xsd:enumeration value="AUSSTELLUNGSFLAECHE"/>
						<xsd:enumeration value="COWORKING"/><!--$V126-->
						<xsd:enumeration value="SHARED_OFFICE"/><!--$V126-->

					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="einzelhandel">
		<xsd:annotation>
			<xsd:documentation>Objektart / Typ f. Handel</xsd:documentation>
<xsd:documentation xml:lang="en"></xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="handel_typ">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="LADENLOKAL"/>
						<xsd:enumeration value="EINZELHANDELSLADEN"/>
						<xsd:enumeration value="VERBRAUCHERMARKT"/>
						<xsd:enumeration value="EINKAUFSZENTRUM"/>
						<xsd:enumeration value="KAUFHAUS"/><!--$V122-->
						<xsd:enumeration value="FACTORY_OUTLET"/><!--$V122-->
